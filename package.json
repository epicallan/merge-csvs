    "eslint-plugin-react": "^5.1.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.3.3",
    "rimraf": "^2.4.3",
    "webpack": "^1.13.0"
  },
  "author": {
    "name": "Allan",
    "email": "epicallan.al@gmail.com"
  },
